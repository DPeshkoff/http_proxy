{
  "name": "hw1_http_proxy",
  "version": "0.0.1",
  "description": "HW1 simple HTTP proxy",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -r -f ./dist",
    "test": "npm run build && nyc qunit ./dist/test --filter https_proxy",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "build": "npm run clean && tsc && cp ./config.yml ./dist/",
    "start": "npm run build && node ./dist/src/index.js",
    "server": "node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DPeshkoff/http_proxy.git"
  },
  "keywords": [
    "http",
    "proxy",
    "technopark"
  ],
  "author": "DPeshkoff",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/DPeshkoff/http_proxy/issues"
  },
  "homepage": "https://github.com/DPeshkoff/http_proxy#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.17",
    "chalk": "^4.1.2",
    "express": "^4.17.2",
    "express-rate-limit": "^6.2.1",
    "js-yaml": "^4.1.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/qunit": "^2.11.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nyc": "^15.1.0",
    "qunit": "^2.17.2"
  },
  "nyc": {
    "exclude": [
      "test/"
    ]
  }
}
